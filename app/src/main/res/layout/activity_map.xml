<?xml version="1.0" encoding="utf-8"?>
<excel.tutorial.familytree.custom.ZoomView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/zoomLayout"
    tools:context="excel.tutorial.familytree.view.activity.Map.MapActivity">

    <!--<RelativeLayout
        android:id="@+id/mainView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/white">

    </RelativeLayout>-->

    <ImageView
        android:id="@+id/imv1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_centerInParent="true"
        android:src="@drawable/wntvtefkevm" />

    <ImageView
        android:id="@+id/imv2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_below="@+id/imv1"
        android:layout_marginTop="32dp"
        android:layout_marginStart="56dp"
        android:src="@drawable/portraitcircle" />

    <ImageView
        android:id="@+id/imv3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_below="@+id/imv1"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="56dp"
        android:layout_alignParentEnd="true"
        android:src="@drawable/portraitcircle" />

    <ImageView
        android:id="@+id/imv4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_below="@+id/imv2"
        android:layout_marginTop="32dp"
        android:layout_marginStart="118dp"
        android:src="@drawable/portraitcircle" />

    <ImageView
        android:id="@+id/imv5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_below="@+id/imv3"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="118dp"
        android:layout_alignParentEnd="true"
        android:src="@drawable/portraitcircle" />

</excel.tutorial.familytree.custom.ZoomView>
